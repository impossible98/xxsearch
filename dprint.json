{
    "incremental": true,
    "typescript": {
        "quoteStyle": "preferSingle"
    },
    "json": {
        "indentWidth": 4
    },
    "markdown": {
    },
    "toml": {
    },
    "dockerfile": {
    },
    "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,toml,dockerfile}"],
    "excludes": [
        "**/node_modules",
        "**/*-lock.json",

        "**/dist"
    ],
    "plugins": [
        "https://plugins.dprint.dev/typescript-0.60.0.wasm",
        "https://plugins.dprint.dev/json-0.13.2.wasm",
        "https://plugins.dprint.dev/markdown-0.11.3.wasm",
        "https://plugins.dprint.dev/toml-0.5.3.wasm",
        "https://plugins.dprint.dev/dockerfile-0.2.1.wasm"
    ]
}
